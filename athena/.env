# API Configuration
REACT_APP_API_URL=http://localhost:3000/api

# Assistant Configuration
REACT_APP_DEFAULT_ASSISTANT_ID=default_assistant_id

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth-domain.auth0.com
REACT_APP_AUTH_CLIENT_ID=your-auth-client-id

# Feature Flags
REACT_APP_ENABLE_VOICE_INPUT=false
REACT_APP_ENABLE_FILE_UPLOAD=true

# Logging and Monitoring
REACT_APP_LOG_LEVEL=info

# Performance
REACT_APP_MAX_CONVERSATION_HISTORY=50

# User Interface
REACT_APP_DEFAULT_THEME=light

# Version
REACT_APP_VERSION=$npm_package_version

# Do NOT include any secret keys or sensitive information in this file
# Secrets should be managed securely and not exposed to the client-side application